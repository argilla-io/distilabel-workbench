    "version": "0.2.0",
    "configurations": [
        {
            "name": "Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Function calling dataset:generations",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceFolder}/scripts/function_calling_dataset",
            "args": [
                "--do_generation",
            ]
        },
        {
            "name": "Function calling dataset:expansions",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceFolder}/scripts/function_calling_dataset",
            "args": [
                "--do_expansion",
            ]
        },
        {
            "name": "Function calling dataset: benchmark",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceFolder}/scripts/function_calling_dataset",
            "args": [
                "--do_benchmark",
            ]
        }
    ]
}